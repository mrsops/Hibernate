<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Seguro" table="seguro">
        <id column="idSeguro" name="idSeguro" type="integer"/>
        <property name="nif" type="string"/>
        <property name="nombre" type="string"/>
        <property name="ape1" type="string"/>
        <property name="ape2" type="string"/>
        <property name="edad" type="integer"/>
        
    </class>
</hibernate-mapping>
